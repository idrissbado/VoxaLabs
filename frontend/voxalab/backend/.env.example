# VoxaLab Environment Configuration
# Copy to .env and fill in your API keys

# Mistral AI API Key - Required for coaching AI
# Get from: https://console.mistral.ai/api-keys/
MISTRAL_API_KEY=your_mistral_api_key_here

# ElevenLabs API Key - Required for voice output
# Get from: https://elevenlabs.io/app/api-keys
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Hugging Face Token - For HF Spaces
# Get from: https://huggingface.co/settings/tokens
HUGGINGFACE_API_KEY=your_huggingface_token_here

# Server port
PORT=8000

# CORS Origins - for local development
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000,http://127.0.0.1:3000
